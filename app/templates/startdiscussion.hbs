{{outlet}}
{{#paper-list}}
  {{#paper-subheader}}Existing Discussions{{/paper-subheader}}
  {{#each model as |item|}}
    {{#paper-item class="md-3-line" onClick=(action "goToDiscussion" item.roomName)}}
      <div class="md-list-item-text">
        <h3>{{item.roomName}}</h3>
      </div>
    {{/paper-item}}
  {{/each}}
{{paper-divider}}
{{/paper-list}}
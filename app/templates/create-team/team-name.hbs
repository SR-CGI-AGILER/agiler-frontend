{{outlet}}


<div class="container-team-name">
{{paper-progress-linear value=33}}
<div class="layout-column">
    <div class="layout layout-align-space-around-center">
        {{paper-input label="teamName" value=teamName onChange=(action (mut teamName))}}
    </div>
    <div class="layout-row layout-align-end-end">
        {{#link-to "create-team.invite-members"}}
            {{#paper-button mini=true warn=true onClick=(action "ok")}}{{paper-icon "keyboard_arrow_right"}}{{/paper-button}}
        {{/link-to}}
    </div>
</div>
</div>

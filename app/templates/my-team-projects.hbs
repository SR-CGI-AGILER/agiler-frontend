{{outlet}}


<div class="container">
{{#paper-autocomplete
  disabled=firstDisabled
    allowClear=true
  search= (action "searchCountries")

  onCreate=(action "searchCountries")
  options=members
  searchField="name"
  selected=selectedCountry

  onSearchTextChange=(action "searchCountries") as |country|
    

  }}
  {{paper-autocomplete-highlight
    label=country.name
    searchText=autocomplete.searchText
    flags="i"}}
{{/paper-autocomplete}}


{{#each model.projects as |project|}}
	{{#paper-list}}
{{#paper-item}}
   {{paper-icon "description"}}
 <p {{action "transitionToProjectView" project}}>{{project.projectName}}</p>



{{#paper-button iconButton=true onClick=(action "deleteProject" project)}}  
 {{paper-icon "delete"}}
       {{/paper-button}}
 
 {{paper-divider}}
 
     {{/paper-item}}
    {{/paper-list}}
    {{/each}}
    <div class="layout-row layout-align-center-end">
{{#paper-button primary=true fab=true onClick=(action "showPromptDialogAction")}}+{{/paper-button}}
{{#if showPromptDialog}}
  {{#paper-dialog fullscreen=fullscreen onClose=(action "closePromptDialog" "cancel") origin=dialogOrigin clickOutsideToClose=true}}
    {{#paper-dialog-content}}
      {{paper-input placeholder="projectName" autofocus=true value=projectName onChange=(action (mut projectName))}}
      {{!-- {{paper-input placeholder="teamName" autofocus=true value=teamName onChange=(action (mut teamName))}} --}}
    {{/paper-dialog-content}}
    {{#paper-dialog-actions class="layout-row"}}
      <span class="flex"></span>
      {{#paper-button primary=true onClick=(action "closePromptDialog" "cancel")}}Cancel{{/paper-button}}
      {{#paper-button primary=true onClick=(action "ok")}}Ok{{/paper-button}}
    {{/paper-dialog-actions}}

  {{/paper-dialog}}
{{/if}}
</div>
</div>